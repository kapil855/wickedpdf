<h1>Blog</h1>

<table border="1" style="text-align: center">
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>		
    </tr>
  </thead>

  <tbody>
    <% @blogs.each do |blog| %>
      <tr>
        <td><%= blog.title %></td>
        <td><%= blog.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>